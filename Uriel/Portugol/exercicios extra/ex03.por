programa{
    funcao inicio(){
        inteiro a,b,c

        escreva("Digite um número: ")
        leia(a)

        escreva("Digite um número: ")
        leia(b)

        se(a == b){
            c = a+b
            escreva(c)
        } senao {
            c = a*b
            escreva(c)
        }

    }
}